{
  "name": "@nestjs-mod/notifications",
  "description": "Notifications module with an error filter, guard, controller, database migrations and rest-sdk for work with module from other nodejs appliaction",
  "version": "1.2.1",
  "keywords": [
    "feature",
    "nestjs",
    "nestjs-mod",
    "mod",
    "files",
    "filter",
    "controller",
    "rest",
    "sdk"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/nestjs-mod/nestjs-mod-contrib/issues"
  },
  "homepage": "https://github.com/nestjs-mod/nestjs-mod-contrib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjs-mod/nestjs-mod-contrib.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "dependencies": {
    "tslib": "^2.3.0",
    "axios": "1.7.2",
    "rxjs": "^7.8.0",
    "ws": "8.18.2",
    "@prisma/client": "^6.9.0",
    "@nestjs/swagger": "^8.1.1",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "@nestjs-mod/common": "2.18.1",
    "@nestjs/common": "^10.4.15",
    "@nestjs-mod/swagger": "1.2.0",
    "@nestjs-mod/prisma-tools": "1.1.0",
    "@nestjs-mod/validation": "1.2.0",
    "@nestjs-mod/webhook": "1.2.1",
    "@nestjs-mod/prisma": "1.16.0",
    "@nestjs/core": "^10.4.15",
    "nestjs-translates": "2.2.5",
    "@prisma/adapter-pg": "^6.9.0",
    "nodemailer": "6.10.0"
  },
  "type": "commonjs",
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "i18n": [
    {
      "scope": "notifications",
      "path": "src/i18n",
      "strategy": "join"
    },
    {
      "scope": "notifications-getText",
      "path": "src/i18n/getText",
      "strategy": "join"
    }
  ]
}
