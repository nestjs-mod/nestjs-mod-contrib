{
    "name": "@nestjs-mod/keyv",
    "description": "Simple key-value storage with support for multiple backends, and a consistent interface for NestJS-mod (Wrapper for https://www.npmjs.com/package/keyv)",
    "version": "1.2.1",
    "keywords": [
        "nestjs",
        "nestjs-mod",
        "modules",
        "mod",
        "cache",
        "keyv",
        "redis",
        "memory",
        "core"
    ],
    "license": "MIT",
    "author": "EndyKaufman <admin@site15.ru>",
    "engines": {
        "node": ">=14",
        "npm": ">=6"
    },
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/nestjs-mod/nestjs-mod-contrib/issues"
    },
    "homepage": "https://github.com/nestjs-mod/nestjs-mod-contrib",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nestjs-mod/nestjs-mod-contrib.git"
    },
    "maintainers": [
        {
            "name": "EndyKaufman",
            "email": "admin@site15.ru"
        }
    ],
    "dependencies": {
        "tslib": "^2.3.0"
    },
    "peerDependencies": {
        "@nestjs/common": "*",
        "@nestjs-mod/common": "*",
        "keyv": "*"
    },
    "peerDependenciesMeta": {
        "class-validator": {
            "optional": true
        },
        "@nestjs/common": {
            "optional": true
        },
        "@nestjs-mod/common": {
            "optional": true
        },
        "keyv": {
            "optional": true
        }
    },
    "dependenciesInfo": {
        "keyv@5.2.3": {
            "docs": true
        }
    },
    "type": "commonjs",
    "main": "./src/index.js",
    "typings": "./src/index.d.ts",
    "i18n": [
        {
            "scope": "keyv",
            "path": "src/i18n",
            "strategy": "join"
        }
    ]
}