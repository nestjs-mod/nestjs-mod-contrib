{
  "name": "prisma",
  "description": "Example application with PM2, docker-compose, postgres and prisma",
  "devScripts": [
    "manual:prepare",
    "docker-compose:start:prisma",
    "db:create",
    "prisma:migrate-deploy:example-prisma",
    "serve:dev:example-prisma"
  ],
  "prodScripts": [
    "manual:prepare",
    "docker-compose:start:prisma",
    "db:create",
    "prisma:migrate-deploy:example-prisma",
    "start:prod:example-prisma"
  ],
  "testsScripts": [
    "test:example-prisma"
  ],
  "dependencies": {
    "pm2": ">=5.3.0",
    "dotenv": ">=16.3.1"
  }
}