{
  "name": "example-terminus",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/example-terminus/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/example-terminus",
        "main": "apps/example-terminus/src/main.ts",
        "tsConfig": "apps/example-terminus/tsconfig.app.json",
        "assets": [
          "apps/example-terminus/src/assets"
        ],
        "webpackConfig": "apps/example-terminus/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "example-terminus:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "example-terminus:build:development"
        },
        "production": {
          "buildTarget": "example-terminus:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/example-terminus/jest.config.ts"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node dist/apps/example-terminus/main.js"
        ],
        "parallel": false
      }
    },
    "db-create": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "./node_modules/.bin/rucken postgres --force-change-username=true --force-change-password=true --root-database-url=${TERMINUS_ROOT_DATABASE_URL} --app-database-url=${TERMINUS_FEAT_DATABASE_URL}"
        ],
        "parallel": false,
        "envFile": "./.env",
        "color": true
      }
    }
  },
  "tags": []
}